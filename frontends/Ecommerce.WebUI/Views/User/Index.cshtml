@model UserDetailViewModel
@{
    Layout = null;
}
<link href="~/lib/bootstrap/dist/css/bootstrap.min.css" rel="stylesheet" />
<div class="container">
    <br /> <br /><br />
    <h3>User Info</h3>
    <div class="alert alert-info">UserId = @Model.Id</div>
    <div class="alert alert-warning">Username = @Model.Username</div>
    <div class="alert alert-info">Name = @Model.Name</div>
    <div class="alert alert-warning">Surname = @Model.Surname</div>
    <div class="alert alert-info">Email = @Model.Email</div>
</div>
