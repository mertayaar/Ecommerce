
@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">
   
@await Component.InvokeAsync("_AdminLayoutHeadComponentPartial")

<body class="sidebar-mini">
    <div>
        <div class="wrapper">

            @await Component.InvokeAsync("_AdminLayoutHeaderComponentPartial")

            @await Component.InvokeAsync("_AdminLayoutSidebarComponentPartial")


            <!-- Content Wrapper. Contains page content -->
            <div class="content-wrapper">
                @await Component.InvokeAsync("_AdminLayoutMainSectionViewBagComponentPartial")

              @RenderBody()

            </div>
            <footer class="main-footer dark-bg">
                <div class="pull-right hidden-xs"> Version 1.0</div>
                Copyright &copy; @DateTime.Now.Year Ecommerce Micro Service. All rights reserved.
            </footer>
        </div>
    </div>
    <!-- wrapper -->
    <!-- jQuery -->
    <script src="~/panel-template/html/dist/js/jquery.min.js"></script>
    <script src="~/panel-template/html/bootstrap/js/bootstrap.min.js"></script>
    <script src="~/panel-template/html/dist/js/ovio.js"></script>
</body>
</html>